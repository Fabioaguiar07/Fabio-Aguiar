#include <stdlib.h> #include <math.h> #include <stdio.h> #include <locale.h> // Funções de pertinência para os níveis de temperatura float baixo(float x) {     if (x <= 0) return 1;     else if (x <= 100) return (100 - x) / 100;     else return 0; } float medio(float x) {     if (x <= 0 || x >= 230) return 0;     else if (x <= 115) return x / 115;     else return (230 - x) / 115; } float alto(float x) {     if (x <= 115) return 0;     else if (x <= 230) return (x - 115) / 115;     else return 1; } // Função para determinar o nível de aquecimento fuzzy char* determinarAquecimento(float temperatura) {     float grauBaixo = baixo(temperatura);     float grauMedio = medio(temperatura);     float grauAlto = alto(temperatura);     // Definindo as regras de inferência fuzzy     float pesoBaixo = grauBaixo;     float pesoMedio = grauMedio;     float pesoAlto = grauAlto;     // Definindo os resultados ponderados     float resultadoBaixo = pesoBaixo;     float resultadoMedio = pesoMedio;     float resultadoAlto = pesoAlto;     // Definindo o nível de aquecimento fuzzy com base nos resultados ponderados     if (resultadoBaixo >= resultadoMedio && resultadoBaixo >= resultadoAlto) {         return "Baixo";     } else if (resultadoMedio >= resultadoBaixo && resultadoMedio >= resultadoAlto) {         return "Médio";     } else {         return "Alto";     } } int main() {     setlocale(LC_ALL, "");  // Definindo a localidade para utilizar caracteres especiais     float temperatura;     printf("Informe a temperatura atual (0 a 230 graus): ");     scanf("%f", &temperatura);     char* nivelAquecimento = determinarAquecimento(temperatura);     printf("Nível de aquecimento: %s\n", nivelAquecimento);     return 0;}
